{
    "format_version": "1.10.0",
    "render_controllers": {
        "controller.render.missouri": {
            "arrays": {
                "textures": {
                    "Array.is_skin_id": [
                        "Texture.usa",
                        "Texture.rs",
                        "Texture.vn",
                        "Texture.pl",
                        "Texture.id",
                        "Texture.jp"
                    ]
                }
            },
            "geometry": "Geometry.default",
            "materials": [
                {
                    "*": "Material.default"
                }
            ],
            "textures": [
                "Array.is_skin_id[query.is_skin_id]"
            ]
        },
        "controller.render.shell_target": {
            "geometry": "Geometry.move",
            "ignore_lighting": true,
            "light_color_multiplier": 1.2,
            "materials": [
                {
                    "*": "Material.move"
                }
            ],
            "overlay_color": {
                "a": 1,
                "b": 1,
                "g": 1,
                "r": 1
            },
            "textures": [
                "Texture.move"
            ]
        }
    }
}